{
  "name": "@kronos-integration/service-systemd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/service.mjs",
  "module": "src/service.mjs",
  "description": "kronos systemd integration",
  "keywords": [
    "systemd",
    "journal",
    "notify"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "pretest": "rollup -c tests/helpers/rollup.config.js",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service.mjs --section=API",
    "lint": "documentation lint src/service.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^8.3.20",
    "config-expander": "^11.0.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.1.0",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.5.2",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "execa": "^4.0.0",
    "node-addon-api": "^2.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-executable": "^1.5.2",
    "rollup-plugin-native": "^1.2.11",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=13.12.0"
  },
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-systemd.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-systemd/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-systemd#readme",
  "native": "${dirname}/${basename}-${os}-${arch}.node",
  "template": {
    "inheritFrom": [
      "arlac77/template-kronos-module",
      "arlac77/template-github"
    ]
  }
}
