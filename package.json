{
  "name": "@kronos-integration/service-systemd",
  "version": "1.5.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/service.mjs",
  "module": "src/service.mjs",
  "description": "kronos systemd integration",
  "keywords": [
    "systemd",
    "journal"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service.mjs --section=API",
    "lint": "documentation lint src/service.mjs",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/service": "^6.1.3",
    "config-expander": "^11.0.6"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "execa": "^3.3.0",
    "node-addon-api": "^1.7.1",
    "rollup": "^1.26.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-native": "0.0.6",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.30"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-systemd.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-systemd/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-systemd#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
