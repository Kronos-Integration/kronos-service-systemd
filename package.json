{
  "name": "@kronos-integration/service-systemd",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/service.mjs",
  "module": "src/service.mjs",
  "description": "kronos systemd integration",
  "keywords": [
    "systemd"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service.mjs --section=API",
    "lint": "documentation lint src/service.mjs",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/service": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "semantic-release": "^15.13.27",
    "node-addon-api": "^1.7.1"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-systemd.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-systemd/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-systemd#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
