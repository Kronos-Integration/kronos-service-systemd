{
  "name": "service-systemd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service.mjs",
  "exports": {
    ".": "./src/service.mjs"
  },
  "description": "kronos systemd integration",
  "keywords": [
    "journal",
    "kronos-service",
    "notify",
    "systemd"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava -s --timeout 5m && ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service.mjs --section=API",
    "lint": "documentation lint ./src/service.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^9.2.12",
    "config-expander": "^11.0.13"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "c8": "^7.3.0",
    "documentation": "^13.0.2",
    "execa": "^4.0.3",
    "node-addon-api": "^3.0.0",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=14.11.0"
  },
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-systemd.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-systemd/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-systemd#readme",
  "native": "${dirname}/${basename}-${os}-${arch}.node",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
