{
  "name": "@kronos-integration/service-systemd",
  "version": "2.1.7",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/service.mjs",
  "module": "src/service.mjs",
  "description": "kronos systemd integration",
  "keywords": [
    "systemd",
    "journal",
    "notify"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service.mjs --section=API",
    "lint": "documentation lint src/service.mjs",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/service": "^8.3.13",
    "config-expander": "^11.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^2.4.0",
    "c8": "^7.0.0",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "execa": "^4.0.0",
    "node-addon-api": "^2.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-executable": "^1.5.2",
    "rollup-plugin-native": "^1.1.4",
    "semantic-release": "^15.14.0"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-systemd.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-systemd/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-systemd#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
